<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lanou.dao.AdressMapper">



    <insert id="insertAdress" parameterType="com.lanou.entity.Adress" >
        INSERT INTO adress(name,email,city,detailAdress,telephone,phone,building, code,u_id) VALUE (#{name},#{email},#{city},#{detailAdress},#{telephone},#{phone},#{building},#{code},#{uId})
    </insert>





</mapper>