<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lanou.dao.CityMapper">

    <resultMap type="com.lanou.entity.City" id="CityResult" >
        <id column="cityid" property="cityid" javaType="int"/>
        <result column="cityname" property="cityname" jdbcType="VARCHAR"/>
        <result column="parentid" property="parentid" javaType="int"/>
    </resultMap>

    <select id="selectCity" parameterType="int" resultMap="CityResult">
        select * from city where parentid =#{cityid}
    </select>

    <resultMap type="com.lanou.entity.Userbirth" id="birthResult" >
        <id column="dateid" property="dateid" javaType="int"/>
        <result column="year" property="year" javaType="int"/>
        <result column="month" property="month" javaType="int"/>
        <result column="day" property="day" javaType="int"/>
    </resultMap>

    <select id="selectYear" resultMap="birthResult">
        select year from Userbirth
    </select>


</mapper>